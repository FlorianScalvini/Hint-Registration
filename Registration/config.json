{
    "train":
    {

        "csv_path": "../data/full_dataset_fused_dataset.csv",
        "epochs": 1000,
        "accumulate_grad_batches": 4,
        "loss": "mse",
        "lam_l": 1,
        "lam_s": 1,
        "lam_m": 0.001,
        "lam_g": 0.005,
        "load": "./Results/version_32/last_model.pth",
        "logger": "log",
        "precision": 32,
        "tensor-cores": false,
        "num_classes" : 3,
        "inshape" : 128
    },

    "model_reg":
    {
      "model" : "AttentionUnet",
      "args" :
      {
          "spatial_dims": 3,
          "in_channels": 2,
          "out_channels": 3,
          "channels": [8, 16, 32],
          "strides": [2, 2]
      }
    }
}
