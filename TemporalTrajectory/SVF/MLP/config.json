{
    "train":
    {
        "csv_path": "/home/florian/Documents/Programs/Hint-Registration/data/full_dataset.csv",
        "t0": 21,
        "t1": 36,
        "epochs": 15000,
        "accumulate_grad_batches": 1,
        "loss": "mse",
        "lam_l": 1,
        "lam_s": 1,
        "lam_m": 0.001,
        "lam_g": 0.005,
        "save": "final_model_",
        "logger": "log",
        "precision": 32,
        "tensor-cores": false,
        "num_classes" : 20,
        "inshape": 128,
        "num_inter_by_epoch": 1
    },

    "model_reg":
    {
      "model" : "AttentionUnet",
      "args" :
      {
          "spatial_dims": 3,
          "in_channels": 2,
          "out_channels": 3,
          "channels": [8, 16, 32],
          "strides": [2, 2]
      }
    },

    "model_svf":
      {
        "model" : "mlp",
        "args" :
        {
            "hidden_size": [1, 32, 32, 32, 1],
            "activation_layer": "ReLU",
            "output_activation": ""
        }
      }
}
